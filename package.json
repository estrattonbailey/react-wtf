{
  "name": "",
  "version": "",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parallelshell 'npm run js:dev' 'npm run css:dev'",
    "js:build": "browserify src/js/main.js --debug -p [minifyify --map main.js.map --output main.js.map] > main.js",
    "js:dev": "watchify src/js/main.js -v -d -o main.js",
    "css:build": "node-sass src/scss/main.scss | postcss -u autoprefixer > main.css",
    "css:dev": "onchange 'src/scss/**/*.scss' -- npm run css:build"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "browserify-shim": {},
  "repository": {
    "type": "git",
    "url": "git+ssh://"
  },
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "exorcist": "^0.4.0",
    "minifyify": "^7.3.3",
    "node-sass": "^3.7.0",
    "onchange": "^2.4.0",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.5.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "prismjs": "^1.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  }
}
